        function setTimer(date, obj, prefix) {
            var countDownDate = date.getTime();

            var updateTime = function () {
                var now = new Date().getTime();
                var distance = countDownDate - now;
                var days = Math.floor(distance / (1000 * 60 * 60 * 24));
                if (days < 0) days = 0;
                //var hours = Math.floor(distance / (1000 * 60 * 60));
                var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                if (hours < 0) hours = 0;
                var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
                var seconds = Math.floor((distance % (1000 * 60)) / 1000);
                var msg = days + "d " + hours + "h " + minutes + "m " + seconds + "s ";
                if (days == 0) msg = hours + "h " + minutes + "m " + seconds + "s ";
                if (days == 0 && hours == 0) msg = minutes + "m " + seconds + "s ";
                if (days == 0 && hours == 0 && minutes == 0) msg = seconds + "s ";
                //document.getElementById(id).innerHTML = prefix + msg;
                obj.html(prefix + msg);
                if (distance < 0) {
                    //clearInterval(x);
                    //document.getElementById(id).innerHTML = "Any minute now!";
                }
            };
            updateTime();
            var x = setInterval(updateTime, 1000);
        }
        var lastUpdatedTime = new Date();
        var updateLastUpdate = function () {
            var now = new Date().getTime();
            var distance = now - lastUpdatedTime;
            var seconds = Math.floor(distance / 1000);
            var msg = seconds + "s";
            $('#lastupdated').html('Last Updated : ' + seconds + " seconds ago");
        };
        updateLastUpdate();
        var x = setInterval(updateLastUpdate, 1000);
        const pollingDelay = 30000;
        const refreshTable = (data) => {
            lastUpdatedTime = data.lastUpdate;
            var rownum = 0;
            const rows = [];
            const now = new Date();
            var wastedPlayers = 0;
            var totalPlayers = 0;
            var bugRate = 1000;
            var botRate = 1000;
            var totalRate = 0;

            for (var i = 0; i < data.mn.length; i++) {
                var planet = data.mn[i];
                //var ownerImg = "/img/" + (planet.initialOwner == 'Terminids' ? "terminid.png" : "automaton.png");
                planet.owner = planet.o;
                planet.name = planet.n;
                planet.initialOwner = planetData[slug(planet.name)].initialOwner;
                planet.regenPerSecond = planet.rps;
                planet.players = planet.p;
                planet.liberated = planet.l;
                planet.startTime = planet.s;
                planet.endTime = planet.e;
                planet.rate = planet.r;
                planet.campaign = planet.c;
                planet.completionTime = planet.ct;
                planet.slug = slug(planet.name);
                var ownerImg = "/img/" + (planet.owner == 2 ? "terminid.png" : planet.owner == 3 ? "automaton.png" : planet.owner == 4 ? "illuminate.png" : "unknown.png");
                var typeImg = "/img/attack.png";
                
                var regenRate = (planet.regenPerSecond / 1000000) * 60 * 60 * 100;
                if (planet.owner == 2 && regenRate < bugRate) bugRate = regenRate;
                if (planet.owner == 3 && regenRate < botRate) botRate = regenRate;
                var defense = false;
                var suffix = $('<text>');
                var majorOrder = $('<text>');
                var title = $('<div>');
                var timeToLiberate = planet.calculatedTime;
                var isMajorOrder = false;
                if (data.mo?.pl?.find(p => p == planet.name) != null) {
                    majorOrder.html('<img width="24px" height="24px" src="/img/majororder.png" />');
                    isMajorOrder = true;
                }
                totalPlayers += planet.players;
                //if (!(planet.endTime > 0 && now < planet.endTime))
                totalRate += planet.rate;
                if (planet.owner == 2 && planet.rate > 0.2 && !(planet.endTime > 0 && now < planet.endTime)) totalRate += bugRate;
                if (planet.owner == 3 && planet.rate > 0.2 && !(planet.endTime > 0 && now < planet.endTime)) totalRate += botRate;
                if (planet.endTime > 0 && now < planet.endTime) {
                    typeImg = "/img/defense.png";
                    defense = true;
                    setTimer(new Date(planet.endTime), suffix, ' ');
                }
                else if (planet.campaign > -1 && planet.liberated > 0.1 || (isMajorOrder && planet.owner != 1)) {
                }
                else {
                    wastedPlayers += planet.players;
                    continue;
                }
                title.append('<a style="color:inherit;text-decoration: none;" href="/Planets/' + planet.slug + '"><img width="18px" height="18px" src="' + typeImg + '" /> ' + planet.name + ' <img width="18px" height="18px" src="' + ownerImg + '"/></a>').append(suffix).append(suffix).append(majorOrder)
                var rateIcon = planet.rate < -3 ? "down3" : planet.rate < -1 ? "down2" : planet.rate < -0.2 ? "down1" : planet.rate < 0.2 ? "neutral" : planet.rate < 1 ? "up1" : planet.rate < 3 ? "up2" : "up3";
                var timer = $('<td>');
                timer.html(planet.completionTime);
                var percentPlayers = (100 * planet.players / 89879).toFixed(0);
                const newrow = $('<tr>').append(($('<td>').append(title))).append(($('<td>').html(planet.liberated.toFixed(2) + '% <img width="12px" height="18px" src="/img/' + rateIcon + '.png" /><text class="d-none d-lg-inline"> (' + planet.rate.toFixed(1) + '%/hr)</text>'))).append(($('<td>').html('<text class="d-none d-lg-inline">' + (planet.players / 1000).toFixed(0) + 'K (</text>' + percentPlayers + '%<text class="d-none d-lg-inline">)</text>'))).append(timer);

                var prefix = '';
                if (defense) {
                    if (planet.completionTime > planet.endTime) prefix = '<b>FAILING</b>: ';
                    else prefix = 'SUCCEEDING: ';
                    if (planet.rate == 0) prefix = 'ERROR: ';
                }
                if (planet.rate > 0.2 || defense) {
                    setTimer(new Date(planet.completionTime), timer, prefix);
                } else if (!defense) {
                    if (planet.rate > -0.2)
                        timer.html('Stalemate');
                    else
                        timer.html('<b>LOSING GROUND</b>. Send Reinforcements!');
                }
                newrow[0].dataset.rownum = ++rownum;
                rows.push(newrow);
            }
            $('#enemyforcesrate').html('Terminid Forces : -' + bugRate.toFixed(2) + '%/hr on each planet</br>Automaton Forces : -' + botRate.toFixed(2) + '%/hr on each planet');
            $('#ratetotal').html(totalRate.toFixed(2) + '%/hr');
            $('#playertotal').html((totalPlayers / 1000).toFixed(0) + 'K');
            var percentWasted = (100 * wastedPlayers / totalPlayers).toFixed(0);
            const wastedRow = $('<tr>').append(($('<td>').append('Other Planets**'))).append(($('<td>').html('0%'))).append(($('<td>').html('<text class="d-none d-lg-inline">' + (wastedPlayers / 1000.0).toFixed(0) + 'K (</text>' + percentWasted + '%<text class="d-none d-lg-inline">)</text>'))).append($('<td>'));
            wastedRow[0].dataset.rownum = ++rownum;
            rows.push(wastedRow);
            $('#eventTable tbody').empty().append(rows);
            
            for (var key of Object.keys(planetData)) {
                var slugKey = slug(key);
                var planet = data.mn.find(e=>slug(e.n) == key);
                if (key == "super-earth"){
                    planetData[slugKey]['activeTooltip'].setOpacity(0.9);
                    planetData[slugKey]['activeTooltip'].setContent(planet?.name + '<br>' + planet?.players + ' players');
                    continue;
                }
                planetData[slugKey]['glowCircle'].options.fillOpacity = planetData[slugKey]['glowCircle'].options.fill = planetData[slugKey]['redSemicircle'].options.fillOpacity = planetData[slugKey]['greenSemicircle'].options.fillOpacity = planetData[slugKey]['activeCircle'].options.opacity  = planetData[slugKey]['emptyCircle'].options.opacity = 0;
                planetData[slugKey]['glowCircle'].options.interactive = planetData[slugKey]['redSemicircle'].options.interactive = planetData[slugKey]['greenSemicircle'].options.interactive = planetData[slugKey]['activeCircle'].options.interactive  = planetData[slugKey]['emptyCircle'].options.interactive = false;
                if (planet == null){
                    planetData[slugKey]['emptyTooltip'].setOpacity(0.9);
                    //planetData[slugKey]['emptyTooltip'].setContent(key);
                    planetData[slugKey]['emptyCircle'].options.interactive = true;
                    planetData[slugKey]['emptyCircle'].options.fillOpacity = 0.5;
                    planetData[slugKey]['emptyCircle'].options.opacity = 1;
                    //var io = planetData[slugKey]['initialOwner'];
                   // var io = planet.owner;
                   // console.log(io);
                    //planetData[slugKey]['emptyCircle'].options.color = io == 'SuperEarth' ? 'blue' : io == 'Terminids' ? 'orange' : io == 'Automatons' ? 'red' : 'lightpurple';
                    planetData[slugKey]['emptyCircle'].redraw();
                    continue;
                }
                var isMajorOrder = false;
                if (data.mo?.pl?.find(p => p == planet.name) != null) {
                    planetData[slugKey]['glowCircle'].options.fillOpacity = planetData[slugKey]['glowCircle'].options.opacity = 1;
                }
                planetData[slugKey]['glowCircle'].redraw();
                if (planet.endTime > 0 || planet.campaign > -1)
                {
                    planetData[slugKey]['campaignTooltip'].setOpacity(0.9);
                    planetData[slugKey]['redSemicircle'].options.interactive = planetData[slugKey]['redSemicircle'].options.interactive = true;
                    planetData[slugKey]['redSemicircle'].options.fillOpacity = planetData[slugKey]['redSemicircle'].options.opacity = 1;
                    if (planet.liberated > 0.1){
                        planetData[slugKey]['greenSemicircle'].options.opacity = planetData[slugKey]['greenSemicircle'].options.fillOpacity = 1;
                        planetData[slugKey]['greenSemicircle'].options.stopAngle = planet.liberated * 360 / 100;
                    }
                    planetData[slugKey]['greenSemicircle'].redraw();
                    planetData[slugKey]['redSemicircle'].redraw();
                }
                planetData[slugKey]['activeCircle'].options.color = planetData[slugKey]['emptyCircle'].options.color = planet.owner == 1 ? 'blue' : planet.owner == 2 ? 'orange' : planet.owner == 3 ? 'red' : 'lightpurple';
                
                if (planet.endTime > 0)
                {
                    var timePct = 100 * (now - planet.startTime) / (planet.endTime - planet.startTime);
                    var tooltip = '<b>' + planet.name + '</b><br>' + planet.liberated.toFixed(4) + '%<br>' + planet.players + ' players<br>Rate: ' + planet.rate.toFixed(2) + '%/hr<br>End at ' + new Date(planet.endTime).toLocaleString() + ' (' + timePct.toFixed(2) + ' %)';
                    planetData[slugKey]['campaignTooltip'].setContent(tooltip);
                }
                else if (planet.campaign > -1)
                {
                    var regenRate = (planet.regenPerSecond / 1000000) * 60 * 60 * 100;
                    var playerForces = planet.rate + regenRate;
                    var tooltip = '<b>' + planet.name + '</b><br>' + planet.liberated.toFixed(4) + '%<br>' + planet.players + ' players<br>Super Earth Forces: ' + 'Negligible' + '<br>Enemy Forces : -' + ((planet.regenPerSecond / 1000000) * 60 * 60 * 100).toFixed(2) + '%/hr';
                    if (planet.liberated > 0.1 )
                        tooltip = '<b>' + planet.name + '</b><br>' + planet.liberated.toFixed(4) + '%<br>' + planet.players + ' players<br>Super Earth Forces: ' + playerForces.toFixed(2) + '%<br>Enemy Forces : -' + ((planet.regenPerSecond / 1000000) * 60 * 60 * 100).toFixed(2) + '%/hr<br>Rate: ' + planet.rate.toFixed(2) + '%/hr';
                    planetData[slugKey]['campaignTooltip'].setContent(tooltip);
                }
                else if (planet.players > 0)
                {
                    planetData[slugKey]['activeCircle'].options.interactive = true;
                    planetData[slugKey]['activeTooltip'].setOpacity(0.9);
                    planetData[slugKey]['activeTooltip'].setContent(planet.name + '<br>' + planet.players + ' players');
                    planetData[slugKey]['activeCircle'].options.fillOpacity = 0.5;
                    planetData[slugKey]['activeCircle'].options.opacity = 1;
                    planetData[slugKey]['activeCircle'].redraw();
                }
            }

            setTimeout(fetchData, pollingDelay);
        }
        function fetchData() {
            if (document.hidden === true) {
                setTimeout(fetchData, 500);
                return;
            }
            url = '/update.json';
            $.getJSON(url, function (data) { refreshTable(data) });
        }

        document.addEventListener("DOMContentLoaded", (event) => {
            fetchData();
        });

        campaign.addTo(map);
        majorOrderLayer.addTo(map);
        personalOrdersLayer.addTo(map);
        warStatsLayer.addTo(map);
        //connections.addTo(map);
       //activePlayers.addTo(map);
       // others.addTo(map);
       
        var overlayMaps = {};
        overlayMaps['campaign'] = campaign;
        overlayMaps['active players'] = activePlayers;
        overlayMaps['empty'] = others;
        overlayMaps['supply lines'] = connections;
        overlayMaps['major order'] = majorOrderLayer;
        overlayMaps['personal orders'] = personalOrdersLayer;
        overlayMaps['war stats'] = warStatsLayer;
        var layerControl = L.control.layers(null, overlayMaps).addTo(map);
            L.control.custom({ id: 'warstats', position: 'topleft', content: '<div><p>Mission Win Rate: 90%, 130.79 M / 144.34 M<br>Bug Kills: 13.30 B<br>Bot Kills: 5.39 B<br>Bullet Acc : 100%, 125.70 B / 119.95 B<br>Deaths: 567.65 M<br>Team Kills: 75.81 M</p>', style: { margin: '10px', padding: '0px 0 0 0', width: '400px', 'text-align': 'left', 'pointer-events': 'none' } }).addTo(map);
            L.control.custom({ id: 'personalorder', position: 'bottomleft', content: '<div><p><b>PERSONAL ORDER</b></br>unk<br>Rewards 15 Medals<br>Time Left 00hr 08m', style: { 'text-align': 'left', 'pointer-events': 'none' } }).addTo(map);
        map.on('layeradd', function (e) {
            if (e.layer == majorOrderLayer) {
                $('#majororder').show();
            }
            if (e.layer == personalOrdersLayer) {
                $('#personalorder').show();
            }
            if (e.layer == warStatsLayer) {
                $('#warstats').show();
            }
        });
        map.on('layerremove', function (e) {
            if (e.layer == majorOrderLayer) {
                $('#majororder').hide();
            }
            if (e.layer == personalOrdersLayer) {
                $('#personalorder').hide();
            }
            if (e.layer == warStatsLayer) {
                $('#warstats').hide();
            }
        });
    </script>
</div>
        </main>
    </div>

